{
  "name": "challenge-league",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:prod": "cp prisma/schema.production.prisma prisma/schema.prisma && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "prisma generate && prisma db push && npm run db:seed",
    "db:prod-fresh": "cp prisma/schema.production.prisma prisma/schema.prisma && prisma generate && prisma db push --force-reset && tsx prisma/seed.ts",
    "db:prod-migrate": "cp prisma/schema.production.prisma prisma/schema.prisma && prisma generate && prisma db push",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.12.0",
    "@types/bcryptjs": "^2.4.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "next": "^14.2.16",
    "next-auth": "^4.24.11",
    "prisma": "^6.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.16",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  }
}
